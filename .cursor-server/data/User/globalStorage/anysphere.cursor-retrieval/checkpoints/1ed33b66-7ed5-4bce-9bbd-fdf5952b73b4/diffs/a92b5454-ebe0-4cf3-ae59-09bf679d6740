aders","USER_AGENT=${userAgent}","ACCEPT_LANGUAGE=en-US,en;q=0.9","","# API Config","FEED_LIMIT=16","FEED_CUT=nf2_latest","`;","","  fs.writeFileSync(envFile, envContent);","  console.log('‚úÖ .env file created successfully');","  console.log('üîë Extracted credentials:');","  console.log(`   Bearer Token: ${bearerToken.substring(0, 50)}...`);","  console.log(`   Session Token: ${sessionMatch ? sessionMatch[1].substring(0, 50) + '...' : 'NOT FOUND'}`);","  console.log(`   CF Clearance: ${cfClearanceMatch ? cfClearanceMatch[1].substring(0, 50) + '...' : 'NOT FOUND'}`);","  console.log(`   OAI DID: ${oaiDidMatch ? oaiDidMatch[1] : 'NOT FOUND'}`);","  ","} catch (error) {","  console.error('‚ùå Error extracting credentials:', error.message);","  process.exit(1);","}",""],"tokenizedAddedLines":[1000000,1000001,1000002,1000003,1000004,1000001,1000005,1000006,1000001,1000007,1000008,1000009,1000010,1000011,1000012,1000009,1000013,1000014,1000015,1000009,1000016,1000017,1000018,1000019,1000020,1000021,1000009,1000022,1000023,1000024,1000009,1000025,1000026,1000001,1000027,1000028,1000029,1000030,1000031,1000032,1000001,1000033,1000034,1000035,1000001,1000036,1000037,1000038,1000039,1000001,1000040,1000041,1000042,1000043,1000044,1000045,1000046,1000009,1000047,1000048,1000049,1000050,1000001]}],"gitInfo":{"noRepoFound":true},"kind":"KIND_ADDED"}